                                <xsl:element name="{$hms-namespace-prefix}:SubSpecialty"></xsl:element>
                            </xsl:element>
                        </xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:CSRNumber"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:CSRState"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:SurescriptsProviderId"></xsl:element>  
                        <xsl:element name="{$hms-namespace-prefix}:CoveredReceipientId"></xsl:element>
                    </xsl:element>
                </xsl:element>
            </xsl:element>
         </xsl:element>
      </xsl:template>

   </xsl:stylesheet>

</syn:localEntry>