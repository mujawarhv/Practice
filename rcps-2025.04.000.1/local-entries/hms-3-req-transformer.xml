                        <xsl:element name="{$hms-namespace-prefix}:Fax"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:Licenses">
                            <xsl:element name="{$hms-namespace-prefix}:License">
                                <xsl:element name="{$hms-namespace-prefix}:Type"></xsl:element>
                                <xsl:element name="{$hms-namespace-prefix}:Number">
                                    <xsl:value-of select="//RxcomPrescriberQueryRequest/Query/stateLicenseNum"/>
                                </xsl:element>
                                <xsl:element name="{$hms-namespace-prefix}:State"></xsl:element>
                            </xsl:element>
                        </xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:NPINumber">
                            <xsl:value-of select="//RxcomPrescriberQueryRequest/Query/npiNum"/>
                        </xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:DEANumbers">
                            <xsl:element name="{$hms-namespace-prefix}:DEANumber">
                                <xsl:value-of select="substring(//RxcomPrescriberQueryRequest/Query/deaNum,1,9)"/>
                            </xsl:element>
                        </xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:Specialties">
                            <xsl:element name="{$hms-namespace-prefix}:Specialty">
