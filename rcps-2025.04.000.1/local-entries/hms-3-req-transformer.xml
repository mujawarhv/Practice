<syn:localEntry xmlns:syn="http://ws.apache.org/ns/synapse" key="hms-3-req-transformer" type="1">
   <xsl:stylesheet version="2.0" xmlns:s="http://schemas.xmlsoap.org/soap/envelope/" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:urn="urn:lnrisk:ws:wshealthcare:@ver=3.17">

      <xsl:output method="xml" omit-xml-declaration="yes" indent="yes" />

      <xsl:variable name="hms-namespace-prefix">urn</xsl:variable>
      <xsl:variable name="hms-soapenv-envelope">xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/"</xsl:variable>
      <xsl:variable name="hms-urn-prefix">xmlns:urn="urn:lnrisk:ws:wshealthcare:@ver=3.17"</xsl:variable>

      <xsl:template match="/">
