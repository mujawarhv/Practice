                            <xsl:element name="{$hms-namespace-prefix}:StreetAddress1">100 LEXINGTON STREET</xsl:element>
                            <xsl:element name="{$hms-namespace-prefix}:City">FORT WORTH</xsl:element>
                            <xsl:element name="{$hms-namespace-prefix}:State">TX</xsl:element>
                            <xsl:element name="{$hms-namespace-prefix}:Zip5">76102</xsl:element>
