                     <xsl:element name="{$hms-namespace-prefix}:IncludeDEA">1</xsl:element>
                     <xsl:element name="{$hms-namespace-prefix}:IncludeNPI">1</xsl:element>
                     <xsl:element name="{$hms-namespace-prefix}:IncludeUPIN">0</xsl:element>
                     <xsl:element name="{$hms-namespace-prefix}:IncludeMedSchool">0</xsl:element>
                     <xsl:element name="{$hms-namespace-prefix}:IncludeMedicaid">1</xsl:element>
                     <xsl:element name="{$hms-namespace-prefix}:IncludeCSR">0</xsl:element>
                     <xsl:element name="{$hms-namespace-prefix}:IncludeAffiliations">0</xsl:element>
                     <xsl:element name="{$hms-namespace-prefix}:IncludeABMSBoardCertifiedSpecialty">0</xsl:element>
