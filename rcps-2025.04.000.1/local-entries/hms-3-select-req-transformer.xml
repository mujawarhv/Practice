                        <xsl:element name="{$hms-namespace-prefix}:UnitNumber"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:StreetAddress1"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:StreetAddress2"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:City"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:State"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:Zip5"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:Zip4"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:Country"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:PostalCode"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:StateCityZip"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:Latitude"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:Longitute"></xsl:element>
