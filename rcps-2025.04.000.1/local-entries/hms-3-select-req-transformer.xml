                        </xsl:element>
                    </xsl:element>
                    <xsl:element name="{$hms-namespace-prefix}:Specialties">
                        <xsl:element name="{$hms-namespace-prefix}:Specialty">
                           <xsl:element name="{$hms-namespace-prefix}:PractitionerType"></xsl:element>
                           <xsl:element name="{$hms-namespace-prefix}:Specialty"></xsl:element>
                           <xsl:element name="{$hms-namespace-prefix}:SubSpecialty"></xsl:element>
