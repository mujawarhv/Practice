                  </xsl:element>
                  <xsl:element name="{$hms-namespace-prefix}:ReportBy">
                     <xsl:element name="{$hms-namespace-prefix}:LNPID">
                        <xsl:value-of select="//RxcomPrescriberSelectRequest/Select/rcpsIdNum"/>
                     </xsl:element>
                     <xsl:element name="{$hms-namespace-prefix}:UniqueId"></xsl:element>
                     <xsl:element name="{$hms-namespace-prefix}:ProviderName">
                        <xsl:element name="{$hms-namespace-prefix}:Full"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:First"></xsl:element>
                        <xsl:element name="{$hms-namespace-prefix}:Last"></xsl:element>
