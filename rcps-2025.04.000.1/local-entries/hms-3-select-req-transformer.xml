                    <xsl:element name="{$hms-namespace-prefix}:SurescriptsProviderId"></xsl:element>
                    <xsl:element name="{$hms-namespace-prefix}:CoveredRecipientId"></xsl:element>
                  </xsl:element>
               </xsl:element>
            </xsl:element>
         </xsl:element>   
      </xsl:template>

   </xsl:stylesheet>

</syn:localEntry>
